{
  "name": "manifests",
  "main": "build/main.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "cdk8s:import": "cdk8s import",
    "kustomize:update": "kustomize edit remove resource 'dist/*' && kustomize edit add resource 'dist/*.yaml'",
    "cdk8s:synth": "cdk8s synth && npm run kustomize:update"
  },
  "dependencies": {
    "cdk8s": "^2",
    "cdk8s-plus-27": "^2",
    "constructs": "^10",
    "type-fest": "^4.2.0",
    "zod": "^3"
  },
  "devDependencies": {
    "cdk8s-cli": "^2.*"
  }
}