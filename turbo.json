{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "build/**"
            ]
        },
        "manifests#pulumi:up": {
            "dependsOn": [
                "build"
            ],
            "outputs": [
                "rendered/**"
            ]
        },
        "bootstrap#pulumi:up": {
            "dependsOn": [
                "build",
                "manifests#pulumi:up"
            ]
        }
    }
}